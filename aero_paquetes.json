  "components" : {
    "schemas" : {
      "Date" : {
        "pattern" : "^[0-9]{4}\\-((0[1-9]|1[0-2])){1}-([012][0-9]|3[01])$",
        "type" : "string",
        "example" : "2017-10-12"
      },
      "Time" : {
        "pattern" : "(?:[01][0-9]|2[0-3])[0-5][0-9]",
        "type" : "string",
        "example" : "2031"
      },
      "Base" : {
        "type" : "string",
        "description" : "Enumerativo string que representa la base de la salida.",
        "example" : "DBL",
        "enum" : [ "SGL", "DBL", "TPL" ]
      },
      "CategoryItem" : {
        "required" : [ "name" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string",
            "description" : "Nombre de la categoría.",
            "example" : "Sol y Playa"
          }
        },
        "description" : "Categorización a la que pertenece una salida."
      },
      "ServiceItem" : {
        "required" : [ "detail", "service_kind" ],
        "type" : "object",
        "properties" : {
          "detail" : {
            "type" : "string",
            "description" : "Nombre del servicio",
            "example" : "Excursión FD - Día completo  Regular - Chichén Itzá"
          },
          "service_kind" : {
            "type" : "string",
            "description" : "Tipo del servicio.",
            "default" : "excursion",
            "enum" : [ "admission", "excursion", "generic", "internal aereal", "transfer", "travel_assistance", "visa" ]
          }
        },
        "description" : "Representa a un servicio de Productos Terrestres que forman parte de la salida."
      },
      "TransferItem" : {
        "required" : [ "category", "detail", "from", "service_kind", "to", "type" ],
        "type" : "object",
        "properties" : {
          "detail" : {
            "type" : "string",
            "description" : "Nombre del servicio",
            "example" : "Traslado IN Privado (Desde:Aeropuerto Cancún  Hasta:Hotel Cancún)"
          },
          "service_kind" : {
            "type" : "string",
            "description" : "Tipo del servicio.",
            "example" : "transfer",
            "enum" : [ "admission", "excursion", "generic", "internal aereal", "transfer", "travel_assistance", "visa" ]
          },
          "type" : {
            "type" : "string",
            "example" : "in_out"
          },
          "category" : {
            "type" : "string",
            "example" : "regular"
          },
          "from" : {
            "type" : "string",
            "example" : "Aeropuerto Cancún"
          },
          "to" : {
            "type" : "string",
            "example" : "Hotel Cancún"
          }
        },
        "description" : "Representa a un traslado de Productos Terrestres que forman parte de la salida."
      },
      "IATACode" : {
        "pattern" : "^[A-Z]{3}$",
        "type" : "string",
        "description" : "Codigo de 3 letras que representa univocamente un lugar o aeropuerto.",
        "example" : "CUN"
      },
      "DepartureItem" : {
        "required" : [ "arrival_airport", "arrival_time", "date", "date_arrival", "departure_airport", "departure_time", "direction", "flight_number" ],
        "type" : "object",
        "properties" : {
          "flight_number" : {
            "type" : "string",
            "description" : "Numero identificador del vuelo.",
            "example" : "LA2411"
          },
          "date" : {
            "$ref" : "#/components/schemas/Date"
          },
          "departure_airport" : {
            "$ref" : "#/components/schemas/IATACode"
          },
          "arrival_airport" : {
            "$ref" : "#/components/schemas/IATACode"
          },
          "departure_time" : {
            "$ref" : "#/components/schemas/Time"
          },
          "arrival_time" : {
            "$ref" : "#/components/schemas/Time"
          },
          "date_arrival" : {
            "$ref" : "#/components/schemas/Date"
          },
          "direction" : {
            "type" : "integer",
            "description" : "Indicador del numero de escala dentro del vuelo general.",
            "format" : "int32",
            "example" : 1
          }
        },
        "description" : "Representa una ruta de los cupos aéreos que forman parte de la salida."
      },
      "AccomodationItem" : {
        "required" : [ "category_name", "check_in", "check_out", "city_iata", "name", "nights", "regime", "room_type", "service_kind", "stars" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string",
            "description" : "Nombre del hotel al que pertenece la habitación.",
            "example" : "RIU Cancún"
          },
          "category_name" : {
            "type" : "string",
            "description" : "Nombre de la categoría de la habitación dentro del hotel.",
            "example" : "Doble Estándar Vista Mar (DBMB)"
          },
          "service_kind" : {
            "type" : "string",
            "description" : "Representa al tipo de servicio, siempre es hotel.",
            "example" : "hotel"
          },
          "room_type" : {
            "type" : "string",
            "description" : "Tipo de habitacion de acuerdo a la base de personas.",
            "example" : "double",
            "enum" : [ "single", "double", "triple", "quadruple", "apartment" ]
          },
          "stars" : {
            "type" : "integer",
            "description" : "Cantidad de estrellas del hotel.",
            "format" : "int32",
            "example" : 4,
            "enum" : [ 1, 2, 3, 4, 5 ]
          },
          "regime" : {
            "type" : "string",
            "description" : "Regimen de la tarifa del hotel.",
            "example" : "Todo incluido",
            "enum" : [ "Sólo alojamiento", "Sólo desayuno", "Media pensión", "Pensión completa", "Todo incluido" ]
          },
          "check_in" : {
            "$ref" : "#/components/schemas/Date"
          },
          "check_out" : {
            "$ref" : "#/components/schemas/Date"
          },
          "nights" : {
            "type" : "integer",
            "description" : "Cantidad de noches que se contrata la habitación.",
            "format" : "int32",
            "example" : 8
          },
          "city_iata" : {
            "$ref" : "#/components/schemas/IATACode"
          }
        },
        "description" : "Representa datos de la habitacion de hotel de la salida."
      },
      "PlaceItem" : {
        "required" : [ "country", "iata_code", "name" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string",
            "description" : "Nombre del lugar completo.",
            "example" : "México, Cancún"
          },
          "country" : {
            "type" : "string",
            "description" : "Nombre del país.",
            "example" : "México"
          },
          "iata_code" : {
            "$ref" : "#/components/schemas/IATACode"
          }
        },
        "description" : "Lugar asociado a la salida."
      },
      "ImageItem" : {
        "required" : [ "url" ],
        "type" : "object",
        "properties" : {
          "url" : {
            "type" : "string",
            "description" : "URL de donde se obtiene la imagen.",
            "example" : "https://aero-tur-dev.s3-sa-east-1.amazonaws.com/cotizador/galleries_packages/58/temp2100720181033.jpg?X-Amz-Expires=600&X-Amz-Date=20180720T152436Z&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Credential=AKIAJDOYXBY4JJXICGEA/20180720/sa-east-1/s3/aws4_request&X-Amz-SignedHeaders=host&X-Amz-Signature=ffac7c390ebfd4bcf46006fd1c8bdd3e7b3dbe6c908da41767b3e823609f5cab"
          }
        },
        "description" : "Representa una imagen de la salida."
      },
      "RateItem" : {
        "required" : [ "ars_value", "eur_value", "gbp_value", "usd_value" ],
        "type" : "object",
        "properties" : {
          "ars_value" : {
            "type" : "number",
            "description" : "Precio en pesos.",
            "format" : "float",
            "example" : 90369.46
          },
          "usd_value" : {
            "type" : "number",
            "description" : "Precio en dolares.",
            "format" : "float",
            "example" : 3250.7
          },
          "eur_value" : {
            "type" : "number",
            "description" : "Precio en libras.",
            "format" : "float",
            "example" : 2712.17
          },
          "gbp_value" : {
            "type" : "number",
            "format" : "float",
            "example" : 2338.63
          }
        },
        "description" : "Precio de la salida en distintas monedas."
      },
      "SearchItem" : {
        "required" : [ "accomodation", "available", "base", "base_related_packages", "categories", "departures", "description", "duration", "end_date", "id", "images", "name", "observations", "other_related_packages", "places", "prices", "prices_with_markup", "related_packages", "services", "start_date" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "integer",
            "format" : "int32",
            "example" : 1
          },
          "name" : {
            "type" : "string",
            "example" : "Salida a Cancún"
          },
          "description" : {
            "type" : "string",
            "example" : "Paquete Masivo N°1 - Cupo LA 19Jul18 8 nts - RIU Cancún - Doble Estándar Vista Mar (DBMB)."
          },
          "available" : {
            "type" : "boolean"
          },
          "observations" : {
            "type" : "string",
            "example" : "Salida promocional para Cancun y alrededores."
          },
          "start_date" : {
            "$ref" : "#/components/schemas/Date"
          },
          "end_date" : {
            "$ref" : "#/components/schemas/Date"
          },
          "base" : {
            "$ref" : "#/components/schemas/Base"
          },
          "categories" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/CategoryItem"
            }
          },
          "places" : {
            "$ref" : "#/components/schemas/SearchItem_places"
          },
          "duration" : {
            "type" : "number",
            "format" : "integer",
            "example" : 8
          },
          "services" : {
            "type" : "array",
            "items" : {
              "oneOf" : [ {
                "$ref" : "#/components/schemas/ServiceItem"
              }, {
                "$ref" : "#/components/schemas/TransferItem"
              } ]
            }
          },
          "departures" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/DepartureItem"
