{
"openapi": "3.0.0",
"externalDocs": {
"description": "Encuentre mas información aquí",
"url": "https://github.com/aerolaplata/paquetes/tree/master"
},
"info": {
"title": "Aero Paquetes",
"x-logo": {
  "url": "https://sitio.aero.tur.ar/assets/logo-positive-ac9a94f014542271c698fdd81c08a30f0ae53f50c7eedfe418680a82b797b975.svg"
},
"description": "Aero-Packs es la aplicacion donde Aero S.R.L ofrece todos sus servicios en un único servicio integral.\n\nEste documento debe utilizarse como referencia para utilizar la API de AeroPacks. El mismo, esta en version beta. Pueden haber modificaciones o nueva información puede ser añadida.\n\nSe necesitan credenciales de acceso para el uso de esta API.\n",
"version": "1.0",
"contact": {
"email": "jonatan.sivori@aerolaplata.com.ar"
},
"license": {
"name": "",
"url": ""
}
},
"components": {
"schemas": {
"Date": {
"type": "string",
"pattern": "^[0-9]{4}\\-((0[1-9]|1[0-2])){1}-([012][0-9]|3[01])$",
"example": "2017-10-12"
},
"Time": {
"type": "string",
"pattern": "(?:[01][0-9]|2[0-3])[0-5][0-9]",
"example": "2031"
},
"Base": {
"type": "string",
"description": "Enumerativo string que representa la base de la salida.",
"enum": [
"SGL",
"DBL",
"TPL"
],
"example": "DBL"
},
"CategoryItem": {
"description": "Categorización a la que pertenece una salida.",
"type": "object",
"required": [
"name"
],
"properties": {
"name": {
"type": "string",
"description": "Nombre de la categoría.",
"example": "Sol y Playa"
}
}
},
"ServiceItem": {
"description": "Representa a un servicio de Productos Terrestres que forman parte de la salida.",
"type": "object",
"required": [
"detail",
"service_kind"
],
"properties": {
"detail": {
"type": "string",
"description": "Nombre del servicio",
"example": "Traslado IN Privado (Desde:Aeropuerto Cancún  Hasta:Hotel Cancún)"
},
"service_kind": {
"type": "string",
"description": "Tipo del servicio.",
"enum": [
"admission",
"excursion",
"generic",
"internal aereal",
"transfer",
"travel_assistance",
"visa"
],
"example": "transfer"
}
}
},
"IATACode": {
"description": "Codigo de 3 letras que representa univocamente un lugar o aeropuerto.",
"type": "string",
"pattern": "^[A-Z]{3}$",
"example": "CUN"
},
"DepartureItem": {
"description": "Representa una ruta de los cupos aéreos que forman parte de la salida.",
"type": "object",
"required": [
"flight_number",
"date",
"departure_airport",
"arrival_airport",
"departure_time",
"arrival_time",
"date_arrival",
"direction"
],
"properties": {
"flight_number": {
"type": "string",
"description": "Numero identificador del vuelo.",
"example": "LA2411"
},
"date": {
"$ref": "#/components/schemas/Date"
},
"departure_airport": {
"$ref": "#/components/schemas/IATACode"
},
"arrival_airport": {
"$ref": "#/components/schemas/IATACode"
},
"departure_time": {
"$ref": "#/components/schemas/Time"
},
"arrival_time": {
"$ref": "#/components/schemas/Time"
},
"date_arrival": {
"$ref": "#/components/schemas/Date"
},
"direction": {
"description": "Indicador del numero de escala dentro del vuelo general.",
"type": "integer",
"example": 1
}
}
},
"AccomodationItem": {
"type": "object",
"description": "Representa datos de la habitacion de hotel de la salida.",
"required": [
"name",
"category_name",
"service_kind",
"room_type",
"stars",
"regime",
"check_in",
"check_out",
"nights",
"city_iata"
],
"properties": {
"name": {
"type": "string",
"description": "Nombre del hotel al que pertenece la habitación.",
"example": "RIU Cancún"
},
"category_name": {
"type": "string",
"description": "Nombre de la categoría de la habitación dentro del hotel.",
"example": "Doble Estándar Vista Mar (DBMB)"
},
"service_kind": {
"type": "string",
"description": "Representa al tipo de servicio, siempre es hotel.",
"example": "hotel"
},
"room_type": {
"type": "string",
"description": "Tipo de habitacion de acuerdo a la base de personas.",
"enum": [
"single",
"double",
"triple",
"quadruple",
"apartment"
],
"example": "double"
},
"stars": {
"type": "integer",
"description": "Cantidad de estrellas del hotel.",
"enum": [
1,
2,
3,
4,
5
],
"example": 4
},
"regime": {
"description": "Regimen de la tarifa del hotel.",
"type": "string",
"enum": [
"Sólo alojamiento",
"Sólo desayuno",
"Media pensión",
"Pensión completa",
"Todo incluido"
],
"example": "Todo incluido"
},
"check_in": {
"$ref": "#/components/schemas/Date"
},
"check_out": {
"$ref": "#/components/schemas/Date"
},
"nights": {
"type": "integer",
"description": "Cantidad de noches que se contrata la habitación.",
"example": 8
},
"city_iata": {
"$ref": "#/components/schemas/IATACode"
}
}
},
"PlaceItem": {
"type": "object",
"description": "Lugar asociado a la salida.",
"required": [
"name",
"country",
"iata_code"
],
"properties": {
"name": {
"type": "string",
"description": "Nombre del lugar completo.",
"example": "México, Cancún"
},
"country": {
"type": "string",
"description": "Nombre del país.",
"example": "México"
},
"iata_code": {
"$ref": "#/components/schemas/IATACode"
}
}
},
"ImageItem": {
"type": "object",
"description": "Representa una imagen de la salida.",
"required": [
"url"
],
"properties": {
"url": {
"type": "string",
"description": "URL de donde se obtiene la imagen.",
"example": "https://aero-tur-dev.s3-sa-east-1.amazonaws.com/cotizador/galleries_packages/58/temp2100720181033.jpg?X-Amz-Expires=600&X-Amz-Date=20180720T152436Z&X-Amz-Algorithm=AWS4-HMAC-SHA256&X-Amz-Credential=AKIAJDOYXBY4JJXICGEA/20180720/sa-east-1/s3/aws4_request&X-Amz-SignedHeaders=host&X-Amz-Signature=ffac7c390ebfd4bcf46006fd1c8bdd3e7b3dbe6c908da41767b3e823609f5cab"
}
}
},
"RateItem": {
"type": "object",
"description": "Precio de la salida en distintas monedas.",
"required": [
"ars_value",
"usd_value",
"eur_value",
"gbp_value"
],
"properties": {
"ars_value": {
"type": "number",
"description": "Precio en pesos.",
"format": "float",
"example": 90369.46
},
"usd_value": {
"type": "number",
"description": "Precio en dolares.",
"format": "float",
"example": 3250.7
},
"eur_value": {
"type": "number",
"description": "Precio en libras.",
"format": "float",
"example": 2712.17
},
"gbp_value": {
"type": "number",
"format": "float",
"example": 2338.63
}
}
},
"SearchItem": {
"type": "object",
"required": [
"id",
"name",
"description",
"available",
"observations",
"start_date",
"end_date",
"base",
"categories",
"services",
"departures",
"accomodation",
"places",
"images",
"prices",
"prices_with_markup",
"related_packages",
"base_related_packages",
"other_related_packages"
],
"properties": {
"id": {
"type": "integer",
"example": 1
},
"name": {
"type": "string",
"example": "Salida a Cancún"
},
"description": {
"type": "string",
"example": "Paquete Masivo N°1 - Cupo LA 19Jul18 8 nts - RIU Cancún - Doble Estándar Vista Mar (DBMB)."
},
"available": {
"type": "boolean",
"example": true
},
"observations": {
"type": "string",
"example": "Salida promocional para Cancun y alrededores."
},
"start_date": {
"$ref": "#/components/schemas/Date"
},
"end_date": {
"$ref": "#/components/schemas/Date"
},
"base": {
"$ref": "#/components/schemas/Base"
},
"categories": {
"type": "array",
"items": {
"$ref": "#/components/schemas/CategoryItem"
}
},
"services": {
"type": "array",
"items": {
"$ref": "#/components/schemas/ServiceItem"
}
},
"departures": {
"type": "array",
"items": {
"$ref": "#/components/schemas/DepartureItem"
}
},
"accomodation": {
"type": "array",
"items": {
"$ref": "#/components/schemas/AccomodationItem"
}
},
"places": {
"type": "array",
"items": {
"$ref": "#/components/schemas/PlaceItem"
}
},
"images": {
"type": "array",
"items": {
"$ref": "#/components/schemas/ImageItem"
}
},
"prices": {
"type": "array",
"items": {
"$ref": "#/components/schemas/RateItem"
}
},
"prices_with_markup": {
"type": "array",
"items": {
"$ref": "#/components/schemas/RateItem"
}
},
"related_packages": {
"type": "array",
"items": {
"type": "integer"
},
"example": [
2,
3,
4,
5,
6,
7,
8
]
},
"base_related_packages": {
"type": "array",
"items": {
"type": "integer"
},
"example": [
2
]
},
"other_related_packages": {
"type": "array",
"items": {
"type": "integer"
},
"example": [
3
]
}
}
},
"SearchRelatedItem": {
"type": "object",
"required": [
"id",
"can_sell",
"accomodation",
"images",
"prices",
"prices_with_markup"
],
"properties": {
"id": {
"type": "integer",
"example": 1
},
"can_sell": {
"type": "boolean",
"description": "Booleano que determina si la salida puede venderse o no en base a la visibilidad y disponibilidad de la misma.",
"example": true
},
"accomodation": {
"type": "array",
"items": {
"$ref": "#/components/schemas/AccomodationItem"
}
},
"images": {
"type": "array",
"items": {
"$ref": "#/components/schemas/ImageItem"
}
},
"prices": {
"type": "array",
"items": {
"$ref": "#/components/schemas/RateItem"
}
},
"prices_with_markup": {
"type": "array",
"items": {
"$ref": "#/components/schemas/RateItem"
}
}
}
},
"ReserveDataParam": {
"type": "object",
"required": [
"seller_email",
"agency_id"
],
"properties": {
"seller_email": {
"type": "string",
"description": "Email del vendedor.",
"example": "jonatan.sivori@aerolaplata.com.ar"
},
"agency_id": {
"type": "integer",
"description": "Identificador único de una agencia.",
"example": 180
}
}
},
"PassengerDataParam": {
"type": "object",
"required": [
"first_name",
"last_name",
"document_type",
"document_number",
"birthdate"
],
"properties": {
"first_name": {
"type": "string",
"description": "Nombre del pasajero.",
"example": "Nombre"
},
"last_name": {
"type": "string",
"description": "Apellido del pasajero.",
"example": "Apellido"
},
"document_type": {
"type": "string",
"description": "Tipo de documento del pasajero.",
"enum": [
"DNI",
"VISA",
"PASAPORTE"
],
"example": "DNI"
},
"document_number": {
"type": "string",
"description": "Numero de documento del pasajero.",
"example": "34343434"
},
"birthdate": {
"$ref": "#/components/schemas/Date"
}
}
},
"BookingDataParam": {
"type": "object",
"description": "Informacion necesaria para el booking de una reserva.",
"required": [
"adults_count",
"reserve_data",
"passengers"
],
"properties": {
"adults_count": {
"$ref": "#/components/parameters/adultsCountParam"
},
"reserve_data": {
"$ref": "#/components/parameters/reserveDataParam"
},
"passengers": {
"$ref": "#/components/parameters/passengersDataParam"
}
}
},
"RateDescriptionItem": {
"type": "object",
"description": "Detalle de importe para endpoints de TravelPack.",
"required": [
"rate",
"amount",
"base",
"description"
],
"properties": {
"rate": {
"type": "number",
"format": "float",
"example": 21
},
"amount": {
"type": "number",
"format": "float",
"example": 15.15
},
"base": {
"type": "number",
"format": "float",
"example": 72.13
},
"description": {
"type": "string",
"example": "Gastos administrativos"
}
}
},
"RateDescriptionMinItem": {
"type": "object",
"description": "Detalle de importe para endpoints de TravelPack.",
"required": [
"amount",
"description"
],
"properties": {
"amount": {
"type": "number",
"format": "float",
"example": 15.15
},
"description": {
"type": "string",
"example": "Gastos administrativos"
}
}
},
"PreliquidationItem": {
"type": "object",
"required": [
"total",
"TourismTaxes",
"FiscalTaxes",
"NonCommissionableService",
"commission_amount",
"commissionable_price"
],
"properties": {
"total": {
"type": "integer",
"format": "float",
"example": 1101.48
},
"TourismTaxes": {
"$ref": "#/components/schemas/RateDescriptionItem"
},
"FiscalTaxes": {
"type": "array",
"items": {
"$ref": "#/components/schemas/RateDescriptionItem"
}
},
"NonCommissionableService": {
"type": "array",
"items": {
"$ref": "#/components/schemas/RateDescriptionMinItem"
}
},
"commission_amount": {
"type": "integer",
"format": "float",
"example": 0
},
"commissionable_price": {
"type": "integer",
"format": "float",
"example": 857
}
}
}
},
