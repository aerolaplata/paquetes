"parameters": {
"requestIdParam": {
"in": "path",
"name": "id",
"required": true,
"schema": {
"type": "integer"
},
"description": "Identificador unívoco del elemento que se busca.",
"example": 1
},
"requestReserveIdParam": {
"in": "path",
"name": "reserve_id",
"required": true,
"schema": {
"type": "integer"
},
"description": "Identificador unívoco de una reserva.",
"example": 35048
},
"startDateParam": {
"in": "header",
"name": "start_date",
"description": "Fecha en formato yyyy-mm-dd.",
"required": false,
"schema": {
"$ref": "#/components/schemas/Date"
}
},
"endDateParam": {
"in": "header",
"name": "end_date",
"description": "Fecha en formato yyyy-mm-dd.",
"required": false,
"schema": {
"$ref": "#/components/schemas/Date"
}
},
"adultsCountParam": {
"in": "header",
"name": "adults_count",
"description": "Cantidad de adultos que sirve para la base de las salidas a buscar.",
"required": false,
"schema": {
"type": "integer",
"example": 2
}
},
"locationIdsParam": {
"in": "header",
"name": "location_ids",
"description": "Arreglo de identificadores únicos de destinos para la búsqueda. En mismo formato, acepta un único identificador.",
"required": false,
"schema": {
"type": "array",
"items": {
"type": "string",
"pattern": "^(\\d{1,5})((,\\s*\\d{1,5})*)?"
},
"example": [
"3453, 1234"
]
}
},
"locationNamesParam": {
"in": "header",
"name": "location_names",
"description": "Arreglo de nombres de destinos para la búsqueda. En mismo formato, acepta un único identificador.",
"required": false,
"schema": {
"type": "array",
"items": {
"type": "string",
"example": "Cancun"
},
"example": [
"Cancun",
"Lima"
]
}
},
"visibleParam": {
"in": "header",
"name": "visible",
"description": "Determina si las salidas a buscar tienen que estar o no visibles, la omisión de este paraámetro se toma como `true`.",
"required": false,
"schema": {
"type": "boolean",
"example": false
}
},
"reserveDataParam": {
"in": "header",
"name": "reserve_data",
"description": "Parámetros para gestionar una reserva.",
"required": true,
"schema": {
"$ref": "#/components/schemas/ReserveDataParam"
}
},
"passengersDataParam": {
"in": "header",
"name": "pasengers",
"description": "Información de los pasajeros para los cuales se gestiona una reserva.",
"required": true,
"schema": {
"type": "array",
"items": {
"$ref": "#/components/schemas/PassengerDataParam"
}
